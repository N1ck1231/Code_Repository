<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.repositories.dao.Code_InfoMapper">

    <select id="find_Code_Info" parameterType="String" resultType="com.repositories.dao.Code_Info">
        select * from code_info where watch_count =#{watch_count};
    </select>


</mapper>
